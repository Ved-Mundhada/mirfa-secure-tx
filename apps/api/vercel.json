{
  "version": 2,
  "buildCommand": "cd ../.. && pnpm turbo run build --filter=api...",
  "installCommand": "pnpm install",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/serverless"
    }
  ],
  "functions": {
    "api/serverless.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}